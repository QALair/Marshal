#DataSource
#spring.datasource.url=jdbc:h2:mem:memDb;DB_CLOSE_DELAY=-1
#spring.datasource.url=jdbc:h2:file:./data/fileDb
#spring.datasource.driverClassName=org.h2.Driver not needed, as per researches made, spring can take up the driver from conn url

spring.datasource.url=jdbc:mysql://SERVIDOR_AWS/DB_AWS
spring.datasource.username=MYSQL_USER
spring.datasource.password=MYSQL_PW

#Console
spring.h2.console.enabled=true
spring.h2.console.path=/h2

#JPA
#spring.jpa.database-platform=org.hibernate.dialect.H2Dialect
#spring.jpa.hibernate.ddl-auto=create-drop
spring.jpa.database-platform=org.hibernate.dialect.MySQL5Dialect
spring.jpa.show-sql=true
spring.jpa.properties.hibernate.format_sql=true
logging.level.com.example=DEBUG